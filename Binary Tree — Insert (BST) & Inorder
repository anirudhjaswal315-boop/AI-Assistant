#include <stdio.h>
#include <stdlib.h>

typedef struct T{ int v; struct T *l,*r;} T;

T* newNode(int v){ T* n=malloc(sizeof(T)); n->v=v; n->l=n->r=NULL; return n; }

T* insert(T* root,int v){
    if(!root) return newNode(v);
    if(v < root->v) root->l = insert(root->l,v);
    else root->r = insert(root->r,v);
    return root;
}

void inorder(T* r){ if(!r) return; inorder(r->l); printf("%d ",r->v); inorder(r->r); }

int main(){
    T* root=NULL;
    root = insert(root,50); insert(root,30); insert(root,70); insert(root,20); insert(root,40);
    inorder(root); printf("\n");
    return 0;
}
