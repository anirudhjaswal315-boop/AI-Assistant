#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, i, j;
    printf("Enter limit: ");
    scanf("%d", &n);

    int *prime = calloc(n + 1, sizeof(int));

    for(i = 2; i * i <= n; i++)
        if(!prime[i])
            for(j = i * i; j <= n; j += i)
                prime[j] = 1;

    printf("Primes: ");
    for(i = 2; i <= n; i++)
        if(!prime[i]) printf("%d ", i);

    free(prime);
    return 0;
}
