#include <stdio.h>
#include <string.h>
#include <ctype.h>

int isPal(char *s, int l, int r){
    while(l < r){
        if(s[l] != s[r]) return 0;
        l++; r--;
    }
    return 1;
}

int main() {
    char s[500];
    gets(s);

    int i=0, start=0;
    while(1) {
        if(s[i]==' ' || s[i]=='\0'){
            if(isPal(s, start, i-1)){
                for(int k=start;k<i;k++) printf("%c", s[k]);
                printf(" (Palindrome)\n");
            }
            start = i+1;
        }
        if(s[i]=='\0') break;
        i++;
    }
}
