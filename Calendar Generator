#include <stdio.h>

int zeller(int d,int m,int y){
    if(m<3){ m+=12; y--; }
    int K=y%100, J=y/100;
    int h = (d + (13*(m+1))/5 + K + K/4 + J/4 + 5*J) % 7;
    return (h+6)%7; // 0=Sunday
}

int daysInMonth(int m,int y){
    if(m==2) return (y%400==0)||(y%4==0 && y%100!=0)?29:28;
    if(m==4||m==6||m==9||m==11) return 30;
    return 31;
}

int main(){
    int m,y; scanf("%d %d",&m,&y);
    printf("Sun Mon Tue Wed Thu Fri Sat\n");
    int start = zeller(1,m,y);
    int dim = daysInMonth(m,y);
    for(int i=0;i<start;i++) printf("    ");
    for(int d=1;d<=dim;d++){
        printf("%3d ", d);
        if((start + d) % 7 == 0) printf("\n");
    }
    printf("\n");
    return 0;
}
