#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main(){
    char s[256]; scanf("%s", s);
    int len=strlen(s), hasUpper=0, hasLower=0, hasDigit=0, hasSym=0;
    for(int i=0;i<len;i++){
        if(isupper(s[i])) hasUpper=1;
        else if(islower(s[i])) hasLower=1;
        else if(isdigit(s[i])) hasDigit=1;
        else hasSym=1;
    }
    int score = (len>=8) + hasUpper + hasLower + hasDigit + hasSym;
    printf("Strength: %d/5\n", score);
    return 0;
}
